{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\.(js|es6|jsx)$"],
  "excludes": ["\\.config\\.(js|es6|jsx)$"],
  "plugins": [
    {
      "name": "esdoc2-standard-plugin"
    },
    {
      "name": "esdoc2-ecmascript-proposal-plugin",
      "option": {
        "classProperties": true,
        "objectRestSpread": true
      }
    }
  ]
}
